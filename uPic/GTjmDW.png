<script language="text/template" id="temp-cmt">
    <div class="cm" id="<%= id %>">
        <img src="<%= creator.avatar ? filterXSS(creator.avatar.replace('https://', 'http://')) : 'http://www.maimemo.com/res/images/site/logo3.png' %>" />
        <div class="ctn">
            <p class="name"><%= filterXSS(creator.name || creator.username) %></p>
            <p class="text"><%= filterXSS(content) %></p>
            <p class="date"><%= created_time_fmt %> ・ <span onclick="<%= 'Actions.reply(event)' %>"><span><%= (replied_count || '') + ' 回复' %></span></span>
                <% if (isAdmin || (oauth && creator.oauthid2 === oauth.oauthid)) { %>
                <span style="display: inline-block; padding: 4px 4px 4px 12px;"
                      onclick="Actions.deleteComment(this, '<%= id %>')">删除</span>
                <% } %>
            </p>
            <div data-cid="<%= filterXSS(id) %>" class="like right <%= like.action === ' LIKE ' ? 'selected' : '' %>" onclick="Actions.likeComment(this)">
                <i class="iconfont">&#xe60a;</i>
                <span><%= filterXSS(like_user_count) || 0 %></span>
            </div>

            <div class="admin" style="display: <%= isAdmin ? '' : 'none' %>" data-cid2="<%= filterXSS(id) %>">
                <div class="rev1" style="display: <%= status == 'UNREVIEWED' ? '' : 'none' %>"
                     data-status="<%= status %>">
                    待审核: <span onclick="<%= " Actions.setStatus(this, 'REVIEWED')"%>">通过</span><span onclick="<%= " Actions.setStatus(this,'REJECTED')"%>">拒绝</span>
                    待审核: <span onclick="<%= " Actions.setStatus(this, 'REVIEWED')"%>">通过</span><span onclick="<%= " Actions.setStatus(this,'REJECTED')"%>">拒绝</span>
                </div>
                <div class="rev2" style="display: <%= status != 'UNREVIEWED' ? '' : 'none' %>"
                     data-status="<%= status %>">
                    <% if (status == 'REVIEWED') { %>
                    已通过：<span onclick="<%= " Actions.setStatus(this, 'REJECTED')"%>">拒绝</span>
                    <% } else { %>
                    已拒绝：<span onclick="<%= " Actions.setStatus(this, 'REVIEWED')"%>">通过</span>
                    <% } %>
                </div>
            </div>

            <% if (replies.length > 0) { %>

            <div class="replies">
                <% for(var i = 0; i < replies.length; i++){ var reply = replies[i]; %>
                <div class="reply <%= i<3? '': 'fold' %> <%= id %>" data-reply-id="<%= reply.id %>">
                    <p><%= filterXSS(reply.creator.name || reply.creator.username) %>:</p>
                    <p style="word-break:break-word"><%= filterXSS(reply.content) %></p>
                    <p class="opts">
                        <%= reply.created_time_fmt %> ・ <span onclick="<%= 'Actions.replyReply(event)'%>">回复</span>
                      <% if (isAdmin || (oauth && reply.creator.oauthid2 === oauth.oauthid)) { %>
                      <span onclick="<%= 'Actions.deleteReply(this)' %>">删除</span>
                        <% } %>
                        <% if (isAdmin && reply.status == 'REVIEWED') { %>
                        <span onclick="<%= " Actions.setStatus(this, 'REJECTED', true)"%>" style="color: blue">屏蔽</span>
                        <% } %>
                        <% if (isAdmin && reply.status == 'REJECTED' || reply.status == 'UNREVIEWED') { %>
                        <span onclick="<%= " Actions.setStatus(this, 'REVIEWED', true)"%>" style="color: green">显示</span>
                        <% } %>
                    </p>
                    <div onclick="<%= 'Actions.likeReply(this)' %>"
                         class="reply-like <%= reply.like && reply.like.action === 'LIKE' ? 'selected' : '' %>">
                        <div>
                            <i class="iconfont">&#xe60a;</i>
                            <span><%= reply.like_user_count %></span>
                        </div>
                    </div>
                </div>
                <% } %>
            </div>
            <% } %>
            <% if ( replies.length>3 ) { %>
            <div class="foldOpenDiv">
                <p onclick="<%= 'Actions.foldReply(this, true)' %>"  class="<%= id %> unfoldBtn">
                    展开剩余<%= replies.length-3  %>条评论
                    <span class="iconfont icon-zhankaitubiao"></span></p>
                <p onclick="<%= 'Actions.foldReply(this, false)' %>" class=" <%= id %> foldBtn" style="display:none;">
                    收起所有评论
                    <span class="iconfont icon-shouqitubiao"></span></p>
            </div>
                <% } %>
            </div>
    </div>
</script>

<script language="text/template" id="temp-admin">
    <div class="admin" style="display: <%= isAdmin ? '' : 'none' %>" data-cid2="<%= filterXSS(id) %>">
